  "scripts": {
    "start": "env-cmd -f .env react-scripts start",
    "start:staging": "env-cmd -f .env.staging react-scripts start",
    "start:prod": "env-cmd -f .env.production react-scripts start",
    "build": "env-cmd -f .env react-scripts build",
    "build:prod": "env-cmd -f .env.production react-scripts build",
    "deploy": "env-cmd -f .env react-scripts build && firebase use dev && firebase deploy --only hosting:portal-dev",
    "deploy:beta": "env-cmd -f .env react-scripts build && firebase use dev && firebase hosting:channel:deploy beta --only portal-dev",
    "deploy:prod": "env-cmd -f .env.production react-scripts build && firebase use default && firebase deploy --only hosting:portal-prod",
    "deploy:prod:beta": "env-cmd -f .env.production react-scripts build && firebase use default && firebase hosting:channel:deploy beta --only portal-prod",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
